{
  "name": "angular-webpack-workflow",
  "version": "1.0.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "test": "./node_modules/.bin/mocha ./src/test",
    "testwatch": "./node_modules/.bin/mocha -w ./src/test",
    "start": "npm run build && nodejs server.js"
  },
  "dependencies": {
    "angular": "^1.5.0",
    "angular-ui-bootstrap": "^1.3.3",
    "async-each-series": "^1.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.6",
    "chai": "^3.5.0",
    "file-dialog": "0.0.7",
    "fs": "0.0.2",
    "jade": "^1.11.0",
    "jade-html-loader": "0.0.3",
    "jade-loader": "^0.8.0",
    "klaw": "^1.3.0",
    "mocha": "^2.5.3",
    "oidc-client": "https://github.com/gastate/oidc-client-js/tarball/release",
    "socket.io-client": "^2.0.3"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.22.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.18.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
